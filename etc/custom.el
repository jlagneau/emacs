(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(git-gutter:added-sign "+")
 '(git-gutter:deleted-sign "-")
 '(git-gutter:modified-sign "=")
 '(global-prettify-symbols-mode t)
 '(indent-tabs-mode nil)
 '(org-fold-catch-invisible-edits 'show-and-error nil nil "Customized with use-package org")
 '(prettify-symbols-unprettify-at-point 'right-edge)
 '(tab-width 4))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(default ((t (:inherit default :height 110 :family "Fira Code"))))
 '(custom-state ((t (:background "#b6e63e" :foreground "black"))))
 '(dired-directory ((t (:foreground "deep sky blue"))))
 '(magit-diff-added-highlight ((t (:extend t :background "#9ac334" :foreground "black" :weight bold))))
 '(variable-pitch ((t (:height 110 :family "Noto Sans")))))
